{
	"[markdown]": {
		"editor.codeActionsOnSave": {
			"source.fixAll.markdownlint": "explicit"
		}
	},
	"[svelte]": {
		"editor.defaultFormatter": "svelte.svelte-vscode"
	},
	"cSpell.words": [
		"anthropics",
		"APAC",
		"autodocs",
		"autoincrement",
		"checkmake",
		"clippy",
		"coderabbitai",
		"cppcheck",
		"detekt",
		"docstrings",
		"dotenv",
		"gantt",
		"gitsecret",
		"golangci",
		"htmlhint",
		"inlang",
		"languagetool",
		"libsql",
		"Logpush",
		"luacheck",
		"markuplint",
		"mdsvex",
		"mermaidchart",
		"oklch",
		"oslojs",
		"paraglide",
		"phpcs",
		"phpmd",
		"phpstan",
		"prioritizer",
		"pylint",
		"rubocop",
		"semgrep",
		"shadcn",
		"shellcheck",
		"sqlfluff",
		"swiftlint",
		"totp",
		"viewports",
		"vite",
		"WCAG"
	],
	"files.associations": {
		"*.css": "tailwindcss"
	}
}
