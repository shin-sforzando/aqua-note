{
	"[markdown]": {
		"editor.codeActionsOnSave": {
			"source.fixAll.markdownlint": "explicit"
		}
	},
	"[svelte]": {
		"editor.defaultFormatter": "svelte.svelte-vscode"
	},
	"cSpell.words": [
		"anthropics",
		"gantt",
		"inlang",
		"libsql",
		"mdsvex",
		"oslojs",
		"paraglide",
		"prioritizer",
		"vite",
		"WCAG"
	]
}
